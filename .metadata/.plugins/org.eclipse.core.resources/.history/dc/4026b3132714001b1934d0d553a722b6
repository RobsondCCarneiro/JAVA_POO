package entidades;

public class DadosBancarios {
	private int idConta;
	private String name;
	private double valor=0, Balance=0;
	
	
	//get e set para acessar as variaveis encapsuladas (privadas) por fora
	public int getIdConta() {
		return idConta;
	}
	public void setIdConta(int idConta) {
		this.idConta = idConta;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public double getvalor() {
		return valor;
	}
	public void setvalor(double valor) {
		this.valor = valor;
	}
	public double getBalance() {
		return Balance;
	}
	public void setBalance(double balance) {
		Balance = balance;
	}
	
	//Agora os metodos
	public void depositado() {
		Balance += valor;
	}
	public void sacado() {
		Balance -= (valor + 5)
	}
	
	
}
